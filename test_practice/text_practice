#..........
def extract_tidied_fields_for_row(row):
   row_split_list = row.split("\t")
   tidied_fields = [field.strip().strip('"') for field in row_split_list]
   return tidied_fields
#or
column_headers = extract_tidied_fields_for_row(rows_data.pop(0))
city_index = column_headers.index('City')
#..........
#-----
cities_list = []

for row in rows_data:
    tidied_fields_row = extract_tidied_fields_for_row(row)
    cities_list.append(tidied_fields_row[city_index])
print(cities_list)
