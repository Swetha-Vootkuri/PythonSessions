#Common_city_list

with open("C:\\Users\\SVI22\\PycharmProjects\\LearnPython\\cities.csv", "r") as fp:
        rows_data = fp.readlines()
        print(type(rows_data))

def extract_tidied_fields(row):
    fields = [field.strip().strip('"') for field in row.split('\t')]
    return fields


headers = extract_tidied_fields(rows_data.pop(0))
city_index = headers.index("City")
state_index = headers.index("State")

rows = [extract_tidied_fields(row) for row in rows_data]


cities = {}
for row in rows:
    city = row[city_index]
    state = row[state_index]
    if city not in cities:
        cities[city] = [state]
    else:
        cities[city].append(state)

common_cities = []
for city, states in cities.items():
    if len(states) > 1:
     common_cities.append(city)

pass
